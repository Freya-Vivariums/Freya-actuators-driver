[Unit]
Description=Freya System Actuators driver
Documentation=https://github.com/Freya-Vivariums
After=multi-user.target dbus.service

[Service]
WorkingDirectory=/opt/Freya/hardware/SystemActuators
Type=simple
ExecStart=/usr/bin/node /opt/Freya/hardware/SystemActuators/build/index.js
Restart=on-failure

[Install]
WantedBy=multi-user.target
